{
  "name": "drupal/mysql56",
  "description": "MySQL 5.6 Driver for Drupal 9.0.",
  "type": "drupal-database-driver",
  "license": "GPL-2.0+",
  "require": {
    "drupal/core": "^9",
    "cweagans/composer-patches": "^1.6.4",
    "oomphinc/composer-installers-extender": "^1.1"
  },
  "extra": {
    "installer-types": [
      "drupal-database-driver"
    ],
    "installer-name": "mysql",
    "patches": {
      "drupal/core": {
        "3118832 - Allow custom database drivers to extend and have the same name as the core ones":
        "https://www.drupal.org/files/issues/2020-03-12/db-prioritize-custom-driver-3118832-16.patch"
      }
    }
  }
}
